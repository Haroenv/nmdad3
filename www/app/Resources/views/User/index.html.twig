{{ include (':default:index.html.twig') }}

{% block body %}

    <table class="table table-hover">
        <thead>
        <tr>
            <th>#</th>
            <th>Username</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for user in users %}
        <tr>
            <th scope="row">{{ user.id }}</th>
             <td>{{ user.username }}</td>
            <td>
                <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                <i class="fa fa-times" aria-hidden="true"></i>
            </td>
        <tr>
            {% endfor %}
        </tbody>
    </table>

{% endblock %}