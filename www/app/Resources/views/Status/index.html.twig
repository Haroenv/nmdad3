{{ include (':default:index.html.twig') }}

{% block body %}

    <table class="table table-hover">
        <thead>
        <tr>
            <th>#</th>
            <th>title</th>
            <th>body</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for status in status %}
        <tr>
            <th scope="row">{{ status.id }}</th>
             <td>{{ status.title }}</td>
             <td>{{ status.body }}</td>
            <td>
                <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                <i class="fa fa-times" aria-hidden="true"></i>
            </td>
        <tr>
            {% endfor %}
        </tbody>
    </table>

{% endblock %}